@model dynamic

@{
    ViewData["Title"] = "Chi tiết hóa đơn";
}

<h2>Chi tiết hóa đơn</h2>

<p>Mã hóa đơn: @Model.InvoiceId</p>
<p>Gmail: @Model.Gmail</p>
<p>Tên người dùng: @Model.UserName</p>
<p>Ngày thanh toán: @Model.PaymentDate</p>
<p>Số ghế: @Model.SeatNumber</p>
<p>Giá: @Model.PaymentAmount</p>
<p>Phim: @Model.FilmName</p>
<p>Phương thức thanh toán: @Model.PaymentMethod</p>
